{
    "name": "ifcastle/swoole-engine",
    "description": "Swoole Engine for IfCastle Application.",
    "type": "ifcastle-engine",
    "license": "MIT",
    "keywords": ["engine", "ifcastle", "swoole", "async", "parallel"],
    "extra": {
        "ifcastle-installer": {
            "package": {
                "name": "swoole_engine",
                "bootloaders": [
                    "IfCastle\\Swoole\\Bootloader"
                ]
            }
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "IfCastle\\Swoole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IfCastle\\Swoole\\": "./tests"
        }
    },
    "require": {
        "ifcastle/application": "^0.8",
        "ifcastle/design-patterns": "^0.4",
        "ifcastle/async-contracts": "^0.5.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.60",
        "amphp/php-cs-fixer-config": "^2.1",
        "phpunit/phpunit": "^11.3"
    }
}
